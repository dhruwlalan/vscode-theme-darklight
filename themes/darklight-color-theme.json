{
  "name": "darklight",
  "type": "dark",
  "colors": {
    //#Scrollbar#//
    "scrollbarSlider.background": "#3E445140",
    "scrollbarSlider.hoverBackground": "#00BFFF40",
    "scrollbarSlider.activeBackground": "#00BFFF80",
    "scrollbar.shadow": "#0000",
    //# Activity Bar#//
    "activityBar.background": "#181818",
    "activityBar.foreground": "#D7DAE0",
    "activityBarBadge.background": "#528bff",
    "activityBarBadge.foreground": "#F8FAFD",
    //# Side Bar#//
    "sideBar.background": "#0f0f0f",
    "sideBar.foreground": "#ffffff",
    "sideBarTitle.foreground": "#00ffff",
    //# Editor Groups & Tabs#//
    "editorGroup.border": "#00ffff70",
    "editorGroupHeader.tabsBackground": "#0e0e0e",
    "editorGroupHeader.tabsBorder": "#191919",
    "tab.activeBackground": "#191919",
    "tab.activeForeground": "#00ffff",
    "tab.inactiveForeground": "#c5c5c5",
    "tab.inactiveBackground": "#0e0e0e",
    "tab.border": "#202020",
    "tab.hoverForeground": "#00ffff",
    "tab.activeBorder": "#00ffff00",
    "tab.activeBorderTop": "#00ffff",
    //# Editor#//
    "editor.background": "#181818",
    "editorLineNumber.foreground": "#00ffff",
    "editorLineNumber.activeForeground": "#00ffff",
    "editorCursor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#212121",
    "editor.selectionBackground": "#00ffff30",
    "editor.selectionHighlightBackground": "#00ffff10",
    "editor.selectionHighlightBorder": "#00ffff60",
    "editorIndentGuide.background": "#202020",
    "editorBracketMatch.background": "#00ffff10",
    "editorBracketMatch.border": "#00ffff80",
    //# Status Bar#//
    "statusBar.background": "#181818",
    "statusBar.foreground": "#00ffff",
    "statusBar.noFolderBackground": "#181818",
    "statusBarItem.hoverBackground": "#00ffff20",
    //# Title Bar#//
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#DEF7F7",
    "panelTitle.activeBorder": "#00ffff",
    "panel.background": "#141414",
    "panelSection.border": "#00ffff",
    //# Others:#//
    "focusBorder": "#00ffff30",
    "foreground": "#FFFFFF",
    "icon.foreground": "#00ffff80"
  },
  ///GENERAL COLORS///
  //: #FFFFFF : plane white | punctuation
  //: #000000 : plane black
  //: #00FFFF : cyan | attribute-name
  //: #FF1493 : deepPink | keywords, storage
  //: #FF69B4 : hotPink
  //: #00FA9A : mediumSpringGreen
  //: #EE82EE : violet | Constant
  //: #FF00FF : magenta
  //: #FFA500 : orange
  //: #FFA07A : lightSalmon
  //: #FFFF00 : yellow
  //: #FF6347 : tomato | invalid
  //: #FF9999 : objectsName
  //: #808080 : spacegray | comments
  //: #9F9C42 : null
  //: #37FFB2 : cssPropName
  //: #FFF969 : cssPropValue
  //: #EEE8AA : operatorLight
  //: #F0E68C : operatorMedium | strings
  //: #FFFF00 : operatorDark
  //: #F1FF7B : classYellow
  //: #F7FFB3 : classYellowLight
  "tokenColors": [
    ///----- COMMON -----///
    //#Comment#//
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#808080"
      }
    },
    //#Invalid#//
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FF6347"
      }
    },
    //#String#//
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#00FA9A"
      }
    },
    ///----- ENV -----///
    //#Key#//
    {
      "scope": ["variable.other.makefile"],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    //#Seperator#//
    {
      "scope": ["punctuation.separator.key-value.makefile"],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF1493"
      }
    },
    //#Value#//
    {
      "scope": ["source.makefile"],
      "settings": {
        "fontStyle": "",
        "foreground": "#00FA9A"
      }
    },
    ///----- JSON -----///
    //#Property Name - top level#//
    {
      "scope": [
        "source.json support.type.property-name.json",
        "source.json punctuation.support.type.property-name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF9999"
      }
    },
    //#Property Name - nested#//
    {
      "scope": [
        "meta.structure.dictionary.value.json support.type.property-name.json",
        "meta.structure.dictionary.value.json punctuation.support.type.property-name"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF9999"
      }
    },
    ///----- HTML -----///
    //#Puntuations#//
    {
      "scope": "text.html",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    //#doctype HTML#//
    {
      "scope": [
        "meta.tag.metadata.doctype.html entity.other.attribute-name.html"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFA500"
      }
    },
    //#Tags#//
    {
      "scope": "entity.name.tag.html",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF1493"
      }
    },
    //#Attributes#//
    {
      "scope": ["text.html entity.other.attribute-name.html"],
      "settings": {
        "fontStyle": "",
        "foreground": "#00ffff"
      }
    },
    //#Entities Symbol#//
    {
      "scope": ["punctuation.definition.entity.html"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFA500"
      }
    },
    //#Entities Constant#//
    {
      "scope": "constant.character.entity",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EE82EE"
      }
    },
    ///----- CSS/SCSS -----///
    //#Puntuations#//
    {
      "scope": ["meta.at-rule.mixin.scss", "source.css"],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    //#SCSS @ rules#//
    {
      "scope": [
        "punctuation.definition.keyword.css",
        "punctuation.definition.keyword.scss",
        "keyword.control"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF1493"
      }
    },
    //#CSS @ rules#//
    {
      "scope": [
        "keyword.control.at-rule.media.scss",
        "keyword.control.at-rule.keyframes.scss",
        "keyword.control.at-rule.fontface.scss",
        "keyword.control.at-rule.media.scss punctuation.definition.keyword.scss",
        "keyword.control.at-rule.keyframes.scss punctuation.definition.keyword.scss",
        "keyword.control.at-rule.fontface.scss punctuation.definition.keyword.scss"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF9999"
      }
    },
    //#Mixin Name#//
    {
      "scope": [
        "meta.at-rule.include.scss entity.name.function.scss",
        "meta.at-rule.mixin.scss entity.name.function.scss"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF9999"
      }
    },
    //#Built-in Functions#//
    {
      "scope": ["support.function.misc.css", "support.function.misc.scss"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFA500"
      }
    },
    //#Variable#//
    {
      "scope": ["variable.css", "variable.scss"],
      "settings": {
        "fontStyle": "",
        "foreground": "#00FFFF"
      }
    },
    //#Null#//
    {
      "scope": ["constant.null.scss"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#9F9C42"
      }
    },
    //#Interpolation#//
    {
      "scope": [
        "punctuation.definition.interpolation.begin.bracket.curly.scss",
        "punctuation.definition.interpolation.end.bracket.curly.scss"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ADFF2F"
      }
    },
    //#Property Name#//
    {
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.media.css",
        "support.type.vendored.property-name.css",
        "meta.at-rule.media.header.css",
        "meta.property-list.scss entity.name.tag.css",
        "meta.property-value.scss support.type.property-name.css",
        "argument.name.scss",
        "meta.property-name.scss",
        "meta.property-name.media-query.scss"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#37FFB2"
      }
    },
    //#Property Value#//
    {
      "scope": [
        "support.constant.font-name.css",
        "support.constant.property-value.css",
        "meta.property-value.scss",
        "meta.property-value.media-query.scss",
        "meta.at-rule.keyframes.scss",
        "entity.name.function.scss"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFF969"
      }
    },
    //#Operator Light#//
    {
      "scope": ["keyword.operator.css"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EEE8AA"
      }
    },
    //#Operator Medium#//
    {
      "scope": [
        "keyword.operator.comparison.scss",
        "source.css keyword.operator.logical",
        "keyword.control.operator.css.scss"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F0E68C"
      }
    },
    //#Operator Dark#//
    {
      "scope": [
        "meta.at-rule.if.scss support.constant.property-value.css",
        "meta.at-rule.else.scss support.constant.property-value.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFFF00"
      }
    },
    //#Keyframes Stops#//
    {
      "scope": "keyword.keyframes.scss",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF1493"
      }
    },
    //#Numbers#//
    {
      "scope": "constant.numeric.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    //#Units#//
    {
      "scope": "keyword.other.unit",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF1493"
      }
    },
    //#Tag Selector#//
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF1493"
      }
    },
    //#Class Selector#//
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.css punctuation.definition.entity.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00BFFF"
      }
    },
    //#ID Selector#//
    {
      "scope": [
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.id.css punctuation.definition.entity.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EE82EE"
      }
    },
    //#Parent Selector &#//
    {
      "scope": ["entity.name.tag.reference.scss"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF1493"
      }
    },
    //#Parent Selector Suffix#//
    {
      "scope": [
        "entity.other.attribute-name.parent-selector-suffix.css punctuation.definition.entity.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#00BFFF"
      }
    },
    //#Pseudo Element Class#//
    {
      "scope": [
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-element.css punctuation.definition.entity.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-class.css punctuation.definition.entity.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F0E68C"
      }
    },
    //#Namespace#//
    {
      "scope": "variable.language.expanded-namespace.scss",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF9999"
      }
    },
    //#Constants#//
    {
      "scope": [
        "support.constant.media.css",
        "constant.other.color.rgb-value.hex.css",
        "constant.other.color.rgb-value.hex.css punctuation.definition.constant.css",
        "support.constant.color.w3c-standard-color-name.css",
        "support.constant.color.w3c-extended-color-name.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EE82EE"
      }
    },
    //#Placeholders#//
    {
      "scope": "entity.other.attribute-name.placeholder.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#6adfff"
      }
    },
    //#Important#//
    {
      "scope": ["keyword.other.important.css", "keyword.other.important.scss"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF6347"
      }
    },
    //#* Universal Selector#//
    {
      "scope": [
        "entity.name.tag.wildcard.scss",
        "entity.name.tag.wildcard.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF1493"
      }
    },
    ///----- JS / TS -----///
    //#Punctuations#//
    {
      "scope": [
        "variable.other.readwrite.js",
        "variable.other.constant.js",
        "meta.brace.round.js",
        "meta.brace.square.js",
        //#TS#//
        "variable.other.readwrite.ts",
        "variable.other.constant.ts",
        "meta.brace.round.ts",
        "meta.brace.square.ts",
        "keyword.operator.type.annotation.ts",
        "punctuation.definition.typeparameters.begin.ts",
        "punctuation.definition.typeparameters.end.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ffffff"
      }
    },
    //#Keywords#//
    {
      "scope": [
        "storage.modifier.js",
        "keyword.control",
        "storage.modifier.async.js",
        "storage.type.js",
        "keyword.operator.expression.typeof.js",
        "keyword.operator.new.js",
        "storage.type.function.js",
        "storage.type.class.js",
        //#TS#//
        "storage.modifier.ts",
        "storage.modifier.async.ts",
        "storage.type.ts",
        "keyword.operator.expression.typeof.ts",
        "keyword.operator.new.ts",
        "storage.type.function.ts",
        "storage.type.class.ts",
        "storage.type.enum.ts",
        "storage.type.type.ts",
        "storage.type.property.ts",
        "storage.type.interface.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF1493"
      }
    },
    //#Classes#//
    {
      "scope": [
        "variable.other.readwrite.alias.js",
        "constant.language.import-export-all.js",
        "meta.function-call.js support.constant",
        //#TS#//
        "variable.other.readwrite.alias.ts",
        "constant.language.import-export-all.ts",
        "meta.function-call.ts support.constant"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#F1FF7B"
      }
    },
    //#Imported Classes#//
    {
      "scope": [
        "variable.other.object.js",
        //#TS#//
        "variable.other.object.ts",
        "entity.name.type.enum.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#F7FFB3"
      }
    },
    //#Orange#//
    {
      "scope": [
        "variable.parameter.js",
        "keyword.control.trycatch.js",
        "punctuation.definition.character-class.regexp",
        //#TS#//
        "variable.parameter.ts",
        "punctuation.decorator.ts",
        "keyword.control.trycatch.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFA500"
      }
    },
    //#Operators Light#//
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment.js",
        "keyword.operator.relational.js",
        //#TS#//
        "keyword.operator.assignment.ts",
        "keyword.operator.relational.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EEE8AA"
      }
    },
    //#Operators Medium#//
    {
      "scope": [
        "keyword.operator.comparison.js",
        "keyword.operator.assignment.compound.js",
        //#TS#//
        "keyword.operator.comparison.ts",
        "keyword.operator.assignment.compound.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F0E68C"
      }
    },
    //#Operators Dark#//
    {
      "scope": [
        "keyword.operator.logical.js",
        "keyword.operator.ternary.js",
        //#TS#//
        "keyword.operator.logical.ts",
        "keyword.operator.ternary.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFFF00"
      }
    },
    //#Constants#//
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character.escape.backslash.regexp",
        "support.constant.json.js",
        //#TS#//
        "support.constant.json.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#EE82EE"
      }
    },
    //#Functions#//
    {
      "scope": [
        "storage.type.function.arrow.js",
        "entity.name.function.js",
        "support.function.js",
        "entity.name.type.js",
        "support.function.json.js",
        "variable.language.super.js",
        "meta.function-call.js support.function",
        //#TS#//
        "storage.type.function.arrow.ts",
        "entity.name.function.ts",
        "support.function.ts",
        "support.function.json.ts",
        "variable.language.super.ts",
        "meta.function-call.ts support.function",
        "meta.decorator.ts variable.other.readwrite.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#00FFFF"
      }
    },
    //#Import Export#//
    {
      "scope": [
        "keyword.control.import.js",
        "keyword.control.from.js",
        "keyword.control.as.js",
        "keyword.control.export.js",
        "keyword.control.default.js",
        //#TS#//
        "keyword.control.import.ts",
        "keyword.control.from.ts",
        "keyword.control.as.ts",
        "keyword.control.export.ts",
        "keyword.control.default.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF69B4"
      }
    },
    //#Objects Keys#//
    {
      "scope": [
        "meta.object-literal.key.js",
        "variable.other.object.property.js",
        "variable.other.property.js",
        //#TS#//
        "meta.object-literal.key.ts",
        "variable.other.object.property.ts",
        "variable.other.property.ts",
        "variable.object.property.ts"
      ],
      "settings": {
        "foreground": "#ff9999"
      }
    },
    //#Module#//
    {
      "scope": [
        "support.type.object.module.js",
        //#TS#//
        "support.type.object.module.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF69B4"
      }
    },
    //#Env Variables#//
    {
      "scope": [
        "variable.other.constant.property.js",
        "variable.other.enummember.ts",
        "variable.other.constant.property.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EE82EE"
      }
    },
    //#Support Variable#//
    {
      "scope": [
        "support.variable.property.js",
        //#TS#//
        "support.variable.property.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff9999"
      }
    },
    //#Destructuring#//
    {
      "scope": [
        "punctuation.definition.binding-pattern.object.js",
        //#TS#//
        "punctuation.definition.binding-pattern.object.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#F7FFB3"
      }
    },
    //#Support Class#//
    {
      "scope": [
        "support.constant.math.js",
        "entity.name.type.module.js",
        "entity.name.type.class.js",
        "support.class",
        //#TS#//
        "entity.name.type.module.ts",
        "entity.name.type.class.ts",
        "entity.other.inherited-class.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F1FF7B"
      }
    },
    //#This#//
    {
      "scope": [
        "variable.language.this.js",
        //#TS#//
        "variable.language.this.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F0E68C"
      }
    },
    //#TS Specific#//
    {
      // types
      "scope": [
        "entity.name.type.ts",
        "support.type.primitive.ts",
        "entity.name.type.alias.ts",
        "entity.name.type.interface.ts",
        "meta.interface.ts entity.other.inherited-class.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#00BFFF"
      }
    },
    ///----- VUE -----///
    //#Punctuations#//
    {
      "scope": ["text.html.vue-html", "source.vue"],
      "settings": {
        "fontStyle": "",
        "foreground": "#ffffff"
      }
    },
    //#Strings#//
    {
      "scope": ["text.html.vue-html string.quoted.double.html"],
      "settings": {
        "fontStyle": "",
        "foreground": "#F0E68C"
      }
    },
    //#Interpolation#//
    {
      "scope": [
        "punctuation.definition.generic.begin.html",
        "punctuation.definition.generic.end.html"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00FA9A"
      }
    },
    //#Generic Html Tags#//
    {
      "scope": ["vue.generic.tag.html"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF1493"
      }
    },
    //#Generic Html Attributes#//
    {
      "scope": ["generic.attribute.html", "entity.other.attribute-name.html"],
      "settings": {
        "fontStyle": "",
        "foreground": "#00ffff"
      }
    },
    //#Vue Tag Reserved#//
    {
      "scope": [
        "vue.tag.template.html",
        "vue.tag.script.html",
        "vue.tag.style.html",
        "vue.tag.reserved.html"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3aeda0"
      }
    },
    //#Vue Tag#//
    {
      "scope": ["vue.tag.html"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00ffc3"
      }
    },
    //#Vue Tag Begin End#//
    {
      "scope": [
        "punctuation.definition.vue.tag.begin.html",
        "punctuation.definition.vue.tag.end.html"
      ],
