{
  "name": "darklight",
  "type": "dark",
  "colors": {
    //#Scrollbar#//
    "scrollbarSlider.background": "#00ffff30",
    "scrollbarSlider.hoverBackground": "#00ffff50",
    "scrollbarSlider.activeBackground": "#00ffff70",
    "scrollbar.shadow": "#0000",
    //# Activity Bar#//
    "activityBar.background": "#181818",
    "activityBar.foreground": "#D7DAE0",
    "activityBarBadge.background": "#528bff",
    "activityBarBadge.foreground": "#F8FAFD",
    //# Side Bar#//
    "sideBar.background": "#0f0f0f",
    "sideBar.foreground": "#ffffff",
    "sideBarTitle.foreground": "#00ffff",
    //# Editor Groups & Tabs#//
    "editorGroup.border": "#00ffff70",
    "editorGroupHeader.tabsBackground": "#0e0e0e",
    "editorGroupHeader.tabsBorder": "#191919",
    "tab.activeBackground": "#191919",
    "tab.activeForeground": "#00ffff",
    "tab.inactiveForeground": "#c5c5c5",
    "tab.inactiveBackground": "#0e0e0e",
    "tab.border": "#202020",
    "tab.hoverForeground": "#00ffff",
    "tab.activeBorder": "#00ffff00",
    "tab.activeBorderTop": "#00ffff",
    //# Editor#//
    "editor.background": "#181818",
    "editorLineNumber.foreground": "#00ffff",
    "editorLineNumber.activeForeground": "#00ffff",
    "editorCursor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#212121",
    "editor.selectionBackground": "#00ffff30",
    "editor.selectionHighlightBackground": "#00ffff10",
    "editor.selectionHighlightBorder": "#00ffff60",
    "editorIndentGuide.background": "#202020",
    "editorBracketMatch.background": "#00ffff10",
    "editorBracketMatch.border": "#00ffff80",
    //# Status Bar#//
    "statusBar.background": "#181818",
    "statusBar.foreground": "#00ffff",
    "statusBar.noFolderBackground": "#181818",
    "statusBarItem.hoverBackground": "#00ffff20",
    //# Title Bar#//
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#DEF7F7",
    "panelTitle.activeBorder": "#00ffff",
    "panel.background": "#141414",
    "panelSection.border": "#00ffff",
    //# Others:#//
    "focusBorder": "#00ffff30",
    "foreground": "#FFFFFF",
    "icon.foreground": "#00ffff80"
  },
  ///COLORS///
  //: #FFFFFF : plane white | punctuation
  //: #000000 : plane black
  //: #00FFFF : cyan | attribute-name
  //: #FF1493 : deepPink | keywords, storage
  //: #FF69B4 : hotPink
  //: #00FA9A : mediumSpringGreen
  //: #EE82EE : violet | Constant
  //: #FF00FF : magenta
  //: #FFA500 : orange
  //: #FFA07A : lightSalmon
  //: #FFFF00 : yellow
  //: #FF6347 : tomato | invalid
  //: #FF9999 : objectsName
  //: #808080 : spacegray | comments
  //: #9F9C42 : null
  //: #37FFB2 : cssPropName
  //: #FFF969 : cssPropValue
  //: #EEE8AA : operatorLight
  //: #F0E68C : operatorMedium | this
  //: #FFFF00 : operatorDark
  //: #F1FF7B : classYellow
  //: #F7FFB3 : classYellowLight
  "tokenColors": [
    ///----- UNOWN -----///
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": ["meta.preprocessor", "entity.name.function.preprocessor"],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "meta.return-type",
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.namespace",
        "entity.other.attribute",
        "entity.name.scope-resolution",
        "entity.name.class",
        "storage.type.numeric.go",
        "storage.type.byte.go",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.uintptr.go",
        "storage.type.error.go",
        "storage.type.rune.go",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "#F1FF7B"
      }
    },
    ///----- COMMON -----///
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    },
    //#Invalid#//
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FF6347"
      }
    },
    //#Puntuations#//
    {
      "scope": ["punctuation", "text", "source"],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    //#Comment#//
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#808080"
      }
    },
    //#String#//
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "source.coffee.embedded"
      ],
      "settings": {
        "foreground": "#00FA9A"
      }
    },
    //#String Interpolation#//
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.interpolation",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#c3ff00"
      }
    },
    //#Constant#//
    {
      "scope": [
        "constant",
        "variable.other.enummember",
        "variable.other.constant.property",
        "punctuation.definition.constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#EE82EE"
      }
    },
    //#Variables#//
    {
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    //#Keywords#//
    {
      "scope": [
        "storage",
        "keyword",
        "variable.language.wildcard.java",
        "entity.name.operator"
      ],
      "settings": {
        "foreground": "#FF1493"
      }
    },
    //#Functions#//
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.powershell variable.other.member",
        "entity.name.operator.custom-literal"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    ///----- ENV/MAKEFILE -----///
    //#Key#//
    {
      "scope": ["variable.other.makefile"],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    //#Seperator#//
    {
      "scope": ["punctuation.separator.key-value.makefile"],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF1493"
      }
    },
    //#Value#//
    {
      "scope": ["source.makefile"],
      "settings": {
        "fontStyle": "",
        "foreground": "#00FA9A"
      }
    },
    ///----- JSON -----///
    //#Property Name - top level#//
    {
      "scope": [
        "support.type.property-name.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF9999"
      }
    },
    //#Property Name - nested#//
    {
      "scope": [
        "meta.structure.dictionary.value.json support.type.property-name.json",
        "meta.structure.dictionary.value.json punctuation.support.type.property-name.begin.json",
        "meta.structure.dictionary.value.json punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF9999"
      }
    },
    ///----- HTML -----///
    //#doctype HTML#//
    {
      "scope": [
        "meta.tag.metadata.doctype.html entity.other.attribute-name.html"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFA500"
      }
    },
    //#Tags#//
    {
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF1493"
      }
    },
    //#Attributes#//
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "fontStyle": "",
        "foreground": "#00ffff"
      }
    },
    //#Entities#//
    {
      "scope": ["punctuation.definition.entity.html"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFA500"
      }
    },
    ///----- CSS/SCSS -----///
    //#Puntuations#//
    {
      "scope": ["source.css"],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    //#SCSS @ rules#//
    {
      "scope": [
        "punctuation.definition.keyword.css",
        "punctuation.definition.keyword.scss",
        "keyword.control"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF1493"
      }
    },
    //#CSS @ rules | Mixin Name#//
    {
      "scope": [
        "keyword.control.at-rule.media.scss",
        "keyword.control.at-rule.keyframes.scss",
        "keyword.control.at-rule.fontface.scss",
        "keyword.control.at-rule.media.scss punctuation.definition.keyword.scss",
        "keyword.control.at-rule.keyframes.scss punctuation.definition.keyword.scss",
        "keyword.control.at-rule.fontface.scss punctuation.definition.keyword.scss",
        "meta.at-rule.include.scss entity.name.function.scss",
        "meta.at-rule.mixin.scss entity.name.function.scss"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF9999"
      }
    },
    //#Built-in Functions#//
    {
      "scope": ["support.function.misc.css", "support.function.misc.scss"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFA500"
      }
    },
    //#Property Name#//
    {
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.media.css",
        "support.type.vendored.property-name.css",
        "meta.at-rule.media.header.css",
        "meta.property-list.scss entity.name.tag.css",
        "meta.property-value.scss support.type.property-name.css",
        "argument.name.scss",
        "meta.property-name.scss",
        "meta.property-name.media-query.scss"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#37FFB2"
      }
    },
    //#Property Value#//
    {
      "scope": [
        "support.constant.font-name.css",
        "support.constant.property-value.css",
        "meta.property-value.scss",
        "meta.property-value.media-query.scss",
        "meta.at-rule.keyframes.scss",
        "entity.name.function.scss"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFF969"
      }
    },
    //#Operator Light#//
    {
      "scope": ["keyword.operator.css"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EEE8AA"
      }
    },
    //#Operator Medium#//
    {
      "scope": [
        "keyword.operator.comparison.scss",
        "source.css keyword.operator.logical",
        "keyword.control.operator.css.scss"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F0E68C"
      }
    },
    //#Operator Dark#//
    {
      "scope": [
        "meta.at-rule.if.scss support.constant.property-value.css",
        "meta.at-rule.else.scss support.constant.property-value.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFFF00"
      }
    },
    //#Class Selector#//
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.css punctuation.definition.entity.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00BFFF"
      }
    },
    //#ID Selector#//
    {
      "scope": [
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.id.css punctuation.definition.entity.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EE82EE"
      }
    },
    //#Parent Selector &#//
    {
      "scope": ["entity.name.tag.reference.scss"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF1493"
      }
    },
    //#Parent Selector Suffix#//
    {
      "scope": [
        "entity.other.attribute-name.parent-selector-suffix.css punctuation.definition.entity.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#00BFFF"
      }
    },
    //#Pseudo Element Class#//
    {
      "scope": [
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-element.css punctuation.definition.entity.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-class.css punctuation.definition.entity.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F0E68C"
      }
    },
    //#Variables#//
    {
      "scope": ["variable.css", "variable.scss"],
      "settings": {
        "fontStyle": "",
        "foreground": "#00FFFF"
      }
    },
    //#Numbers#//
    {
      "scope": "constant.numeric.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    //#Units#//
    {
      "scope": "keyword.other.unit",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF1493"
      }
    },
    //#Keyframes Stops#//
    {
      "scope": "keyword.keyframes.scss",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF1493"
      }
    },
    //#Placeholders#//
    {
      "scope": "entity.other.attribute-name.placeholder.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#6adfff"
      }
    },
    //#Important#//
    {
      "scope": ["keyword.other.important.css", "keyword.other.important.scss"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF6347"
      }
    },
    //#Namespace#//
    {
      "scope": "variable.language.expanded-namespace.scss",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF9999"
      }
    },
    //#* Universal Selector#//
    {
      "scope": [
        "entity.name.tag.wildcard.scss",
        "entity.name.tag.wildcard.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF1493"
      }
    },
    ///----- JS / TS -----///
    //#Punctuations#//
    {
      "scope": [
        "variable.other.readwrite.js",
        "variable.other.constant.js",
        "meta.brace.round.js",
        "meta.brace.square.js",
        //#TS#//
        "variable.other.readwrite.ts",
        "variable.other.constant.ts",
        "meta.brace.round.ts",
        "meta.brace.square.ts",
        "keyword.operator.type.annotation.ts",
        "punctuation.definition.typeparameters.begin.ts",
        "punctuation.definition.typeparameters.end.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ffffff"
      }
    },
    //#Operators Light#//
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment.js",
        "keyword.operator.relational.js",
        //#TS#//
        "keyword.operator.assignment.ts",
        "keyword.operator.relational.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EEE8AA"
      }
    },
    //#Operators Medium#//
    {
      "scope": [
        "keyword.operator.comparison.js",
        "keyword.operator.assignment.compound.js",
        //#TS#//
        "keyword.operator.comparison.ts",
        "keyword.operator.assignment.compound.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F0E68C"
      }
    },
    //#Operators Dark#//
    {
      "scope": [
        "keyword.operator.logical.js",
        "keyword.operator.ternary.js",
        //#TS#//
        "keyword.operator.logical.ts",
        "keyword.operator.ternary.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFFF00"
      }
    },
    //#Classes#//
    {
      "scope": [
        "variable.other.readwrite.alias.js",
        "constant.language.import-export-all.js",
        "meta.function-call.js support.constant",
        //#TS#//
        "variable.other.readwrite.alias.ts",
        "constant.language.import-export-all.ts",
        "meta.function-call.ts support.constant"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#F1FF7B"
      }
    },
    //#Imported Classes | DOM#//
    {
      "scope": [
        "variable.other.object.js",
        //#TS#//
        "variable.other.object.ts",
        "entity.name.type.enum.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#F7FFB3"
      }
    },
    //#ESM | CommonJS#//
    {
      "scope": [
        "keyword.control.import.js",
        "keyword.control.from.js",
        "keyword.control.as.js",
        "keyword.control.export.js",
        "keyword.control.default.js",
        "support.type.object.module.js",
        //#TS#//
        "keyword.control.import.ts",
        "keyword.control.from.ts",
        "keyword.control.as.ts",
        "keyword.control.export.ts",
        "keyword.control.default.ts",
        "support.type.object.module.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF69B4"
      }
    },
    //#Try Catch | parameters#//
    {
      "scope": [
        "variable.parameter.js",
        "keyword.control.trycatch.js",
        "punctuation.definition.character-class.regexp",
        //#TS#//
        "variable.parameter.ts",
        "punctuation.decorator.ts",
        "keyword.control.trycatch.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFA500"
      }
    },
    //#Objects Keys#//
    {
      "scope": [
        "meta.object-literal.key.js",
        "variable.other.object.property.js",
        "variable.other.property.js",
        "support.variable.property.js",
        //#TS#//
        "meta.object-literal.key.ts",
        "variable.other.object.property.ts",
        "variable.other.property.ts",
        "variable.object.property.ts",
        "support.variable.property.ts"
      ],
      "settings": {
        "foreground": "#ff9999"
      }
    },
    //#Destructuring#//
    {
      "scope": [
        "punctuation.definition.binding-pattern.object.js",
        //#TS#//
        "punctuation.definition.binding-pattern.object.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#ebf786"
      }
    },
    //#Support Class#//
    {
      "scope": [
        "support.constant.math.js",
        "entity.name.type.module.js",
        "entity.name.type.class.js",
        "support.class",
        //#TS#//
        "entity.name.type.module.ts",
        "entity.name.type.class.ts",
        "entity.other.inherited-class.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F1FF7B"
      }
    },
    //#This#//
    {
      "scope": [
        "variable.language.this.js",
        //#TS#//
        "variable.language.this.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F0E68C"
      }
    },
    //#TS Specific#//
    {
      // types
      "scope": [
        "entity.name.type.ts",
        "support.type.primitive.ts",
        "entity.name.type.alias.ts",
        "entity.name.type.interface.ts",
        "meta.interface.ts entity.other.inherited-class.ts"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#00BFFF"
      }
    },
    ///----- VUE -----///
    //#Punctuations#//
    {
      "scope": ["text.html.vue-html", "source.vue"],
      "settings": {
        "fontStyle": "",
        "foreground": "#ffffff"
      }
    },
    //#Strings#//
    {
      "scope": ["text.html.vue-html string.quoted.double.html"],
      "settings": {
        "fontStyle": "",
        "foreground": "#F0E68C"
      }
    },
    //#Interpolation#//
    {
      "scope": [
        "punctuation.definition.generic.begin.html",
        "punctuation.definition.generic.end.html"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00FA9A"
      }
    },
    //#Generic Html Tags#//
    {
      "scope": ["vue.generic.tag.html"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF1493"
      }
    },
    //#Generic Html Attributes#//
    {
      "scope": ["generic.attribute.html"],
      "settings": {
        "fontStyle": "",
        "foreground": "#00ffff"
      }
    },
    //#Vue Tag Reserved#//
    {
      "scope": [
        "vue.tag.template.html",
        "vue.tag.script.html",
        "vue.tag.style.html",
        "vue.tag.reserved.html"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3aeda0"
      }
    },
